{
  "name": "constellation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "python3 -m http.server 5173",
    "fix": "npx eslint ./src --fix && prettier ./src --write",
    "build": "npm run fix && npm run build-nofix",
    "build-apps": "/bin/bash build_apps.sh",
    "build-d-ts": "rm -rf types/* && tsc ./src/apps/api.ts --declaration --emitDeclarationOnly --outDir types --lib ES2015",
    "build-nofix": "rm -rf build/* && npx tsc && echo npm run build-d-ts && npm run build-apps && node -e \"console.log(Date.now())\" > build/date.txt",
    "clean": "rm -rf build/*"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@stylistic/eslint-plugin": "4.4.1",
    "eslint": "9.28.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  }
}
